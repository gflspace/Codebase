{
    "name": "qwickservices/cis-laravel",
    "description": "Laravel integration package for QwickServices Contact Integrity System (CIS)",
    "type": "library",
    "keywords": ["laravel", "cis", "contact-integrity", "fraud-detection", "qwickservices"],
    "license": "MIT",
    "authors": [
        {
            "name": "QwickServices",
            "email": "dev@qwickservices.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.5",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/http": "^10.0|^11.0",
        "illuminate/contracts": "^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0|^9.0",
        "mockery/mockery": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "QwickServices\\CIS\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "QwickServices\\CIS\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "QwickServices\\CIS\\CISServiceProvider"
            ],
            "aliases": {
                "CIS": "QwickServices\\CIS\\Facades\\CIS"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
